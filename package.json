{
  "name": "pjsip-node",
  "version": "1.0.0",
  "description": "PJSIP wrapper for Node.js - Similar to baresip-node",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:native && npm run build:js",
    "build:native": "node-gyp rebuild",
    "build:js": "tsc",
    "test": "node dist/test.js",
    "example": "node dist/example.js",
    "clean": "rimraf build dist",
    "prebuild": "npm run clean && node -e \"require('fs').mkdirSync('build',{recursive:true})\"",
    "configure": "node-gyp configure",
    "install": "npm run build",
    "start": "node dist/test.js"
  },
  "keywords": [
    "pjsip",
    "sip",
    "voip",
    "node",
    "addon",
    "baresip",
    "native"
  ],
  "author": "Bhavdeep",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^8.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "build/Release/",
    "README.md",
    "LICENSE"
  ]
}
